[
  {
    "origin": ["*"],
    "method": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "responseHeader": [
      "Content-Type",
      "Access-Control-Allow-Origin",
      "Authorization",
      "X-Requested-With",
      "X-HTTP-Method-Override",
      "Content-Range",
      "Content-Disposition",
      "Content-Length"
    ],
    "maxAgeSeconds": 3600
  }
] 